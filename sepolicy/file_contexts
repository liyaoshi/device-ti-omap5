#Bluettoth tty device
/dev/tty_hci			u:object_r:bluetooth_control:s0

#Console
/dev/ttyS0			u:object_r:console_device:s0

#Graphics
/dev/dri/card0			u:object_r:gpu_device:s0
/dev/dri/controlD64		u:object_r:gpu_device:s0
/dev/dri/renderD128		u:object_r:gpu_device:s0

/system/vendor/bin/pvrsrvctl	u:object_r:pvr_exec:s0
/system/vendor/bin/pvrsrvinit	u:object_r:pvr_exec:s0
